{
  "version": 2,
  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "./api/auto-checkout.js" }
  ],
  "crons": [
    {
      "path": "/api/auto-checkout.js",
      "schedule": "10 11 * * *",
      "timezone": "Asia/Kolkata"
    }
  ]
}
