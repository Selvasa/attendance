{
  "version": 2,
  "builds": [
    { "src": "api/auto-checkout.js", "use": "@vercel/node" },
    { "src": "./index.js", "use": "@vercel/node" }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "cron": [
    {
      "path": "/api/auto-checkout.js",
      "schedule": "50 5 * * *"
    }
  ]
}
